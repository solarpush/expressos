{
  "name": "expressos",
  "version": "1.1.1",
  "description": "CLI tool to create Express services with clean architecture",
  "type": "module",
  "homepage": "https://github.com/solarpush/expressos#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solarpush/expressos.git"
  },
  "bugs": {
    "url": "https://github.com/solarpush/expressos/issues"
  },
  "bin": {
    "expressos": "./dist/index.js",
    "eos": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "publish": "npm publish",
    "release:patch": "npm version patch && git push && git push --tags",
    "release:minor": "npm version minor && git push && git push --tags",
    "release:major": "npm version major && git push && git push --tags"
  },
  "keywords": [
    "cli",
    "express",
    "typescript",
    "clean-architecture",
    "services",
    "api",
    "generator"
  ],
  "author": "ExpressOS Team",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.12",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.6",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}